<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ic_00">

    <application
        android:allowBackup="true"
        android:icon="@drawable/engsoft_logo2_vector"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"

        android:supportsRtl="true"
        android:theme="@style/Theme.IC_00">
        <activity
            android:name=".AddFoodInMyList"
            android:exported="false"
            android:label="Add Food In My List"/>

        <activity
            android:name=".AddFoodInFridge"
            android:exported="false"
            android:label="Add Food In Fridge List"/>
        <activity
            android:name=".AddRecipe"
            android:exported="false"
            android:parentActivityName=".LikedFoods"
            android:label="Add Recipe"/>
        <activity
            android:name=".AddDislikedFoods"
            android:exported="false"
            android:parentActivityName=".DislikedFoods"
            android:label="Add Disliked Food"/>
        <activity
            android:name=".AddLikedFoods"
            android:exported="false"
            android:parentActivityName=".LikedFoods"
            android:label="Add Liked Food"/>
        <activity
            android:name=".Login"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:label="Login"/>
        <activity
            android:name=".MyList"
            android:exported="false"
            android:parentActivityName=".HomePage"
            android:label="Shopping list"/>
        <activity
            android:name=".WhatToEat"
            android:exported="false"
            android:parentActivityName=".HomePage"
            android:label="Meal suggestions"/>
        <activity
            android:name=".InMyFridge"
            android:exported="false"
            android:parentActivityName=".HomePage"
            android:label="My Fridge" />
        <activity
            android:name=".HomePage"
            android:exported="false"
            android:label="Home"/>
        <activity
            android:name=".DislikedFoods"
            android:exported="false"
            android:parentActivityName=".HomePage"
            android:label="Disliked Foods"/>
        <activity
            android:name=".LikedFoods"
            android:exported="false"
            android:parentActivityName=".HomePage"
            android:label="Liked Foods"/>
        <activity
            android:name=".Register"
            android:exported="false"
            android:parentActivityName=".MainActivity"
            android:label="Register">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>